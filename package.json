  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "lint": "eslint src",
